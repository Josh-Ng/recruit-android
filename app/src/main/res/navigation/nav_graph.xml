<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    app:startDestination="@id/transactionListFragment">

    <fragment
        android:id="@+id/transactionListFragment"
        android:name="nz.co.test.transactions.main.presentation.fragments.TransactionsFragment"
        android:label="Transaction List"
        tools:layout="@layout/fragment_transactions">
        <action
            android:id="@+id/action_transactionListFragment_to_transactionDetailFragment"
            app:destination="@id/transactionDetailFragment" />
    </fragment>

    <fragment
        android:id="@+id/transactionDetailFragment"
        android:name="nz.co.test.transactions.main.presentation.fragments.TransactionDetailFragment"
        android:label="Transaction Detail"
        tools:layout="@layout/fragment_transaction_detail" >
        <argument
            android:name="transaction"
            app:argType="nz.co.test.transactions.main.data.models.Transaction" />
    </fragment>
</navigation>
